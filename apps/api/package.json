{
  "name": "@ai-company/api",
  "version": "1.0.0",
  "main": "dist/main.js",
  "description": "",
  "types": "dist/main.d.ts",
  "author": "",
  "license": "ISC",
  "scripts": {
    "lint:fix": "pnpm run lint --fix",
    "lint": "pnpm eslint \"src/**/*.{js,ts,tsx}\" \"cypress/**/*.{js,ts,tsx}\"",
    "build": "tsc -p tsconfig.build.json",
    "build:tests": "tsc -p tsconfig.tests.json",
    "build:runtime": "podman build -f runtime.Dockerfile -t ai-company-runtime:latest .",
    "prebuild": "rm -rf ./dist || true",
    "test": "vitest run --passWithNoTests",
    "test:cov": "vitest run --coverage --passWithNoTests",
    "test:e2e:generate-api": "cy-generate-api http://localhost:5000/swagger-api-json cypress/api-definitions && pnpm run lint:fix",
    "test:e2e": "cypress run -P ./cypress",
    "test:e2e:local": "cypress run -P ./cypress --config baseUrl=http://localhost:5000",
    "test:e2e:open": "cypress open -P ./cypress",
    "test:e2e:open:local": "cypress open -P ./cypress --config baseUrl=http://localhost:5000",
    "start:dev": "NODE_ENV=development pnpm ts-node-dev --compiler ts-patch/compiler --project ./tsconfig.json -r tsconfig-paths/register --respawn --exit-child --files ./src/main",
    "start": "node dist/main",
    "migration:create": "create-migration ./src/db",
    "migration:generate": "generate-migration ./src/db",
    "migration:run": "run-all-migrations ./src/db",
    "migration:revert": "revert-migration ./src/db",
    "seed:create": "NODE_ENV=development create-seed ./src/db",
    "seed:run-all": "NODE_ENV=development run-all-seeds ./src/db"
  },
  "dependencies": {
    "@fastify/static": "^8.3.0",
    "@langchain/core": "^1.0.1",
    "@langchain/langgraph": "^1.0.1",
    "@langchain/langgraph-checkpoint": "^1.0.0",
    "@langchain/openai": "^1.0.0",
    "@nestjs/common": "^11.1.8",
    "@nestjs/core": "^11.1.8",
    "@nestjs/platform-socket.io": "^11.1.7",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.8",
    "@packages/common": "workspace:*",
    "@packages/http-server": "workspace:*",
    "@packages/metrics": "workspace:*",
    "@packages/typeorm": "workspace:*",
    "@tavily/core": "^0.5.12",
    "axios": "^1.13.0",
    "bullmq": "^5.61.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.6.0",
    "dockerode": "^4.0.9",
    "dotenv": "^17.2.3",
    "enum-values": "^1.2.1",
    "fastify": "^5.6.1",
    "ioredis": "^5.8.2",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "nestjs-zod": "^5.0.1",
    "openai": "^6.7.0",
    "outdent": "^0.8.0",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "typeorm": "^0.3.27",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@hey-api/client-axios": "^0.9.1",
    "@nestjs/testing": "^11.1.8",
    "@packages/cypress": "workspace:*",
    "@types/dockerode": "^3.3.45",
    "@types/ioredis": "^5.0.0",
    "@types/lodash": "^4.17.20",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.9.1",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^11.0.0",
    "cypress": "^15.5.0",
    "eslint": "^9.38.0",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^3.3.0",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^5.1.0",
    "typescript": "^5.9.3",
    "typescript-nestjs-swagger-plugin": "^2.0.0",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  }
}
