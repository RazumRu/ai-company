{
  "name": "@ai-company/api",
  "version": "1.0.0",
  "main": "dist/main.js",
  "description": "",
  "types": "dist/main.d.ts",
  "author": "",
  "license": "ISC",
  "scripts": {
    "lint:fix": "pnpm run lint --fix",
    "lint": "pnpm eslint ./src/**",
    "build": "tsc -p tsconfig.build.json",
    "prebuild": "rm -rf ./dist || true",
    "test": "vitest run --passWithNoTests",
    "test:cov": "vitest run --coverage --passWithNoTests",
    "test:e2e:generate-api": "cy-generate-api http://localhost:5000/swagger-api-json cypress/api-definitions && pnpm run lint:fix",
    "test:e2e": "cypress run -P ./cypress",
    "test:e2e:local": "cypress run -P ./cypress --config baseUrl=http://localhost:5000",
    "test:e2e:open": "cypress open -P ./cypress",
    "test:e2e:open:local": "cypress open -P ./cypress --config baseUrl=http://localhost:5000",
    "start:dev": "NODE_ENV=development pnpm ts-node-dev --compiler ts-patch/compiler --project ./tsconfig.json -r tsconfig-paths/register --respawn --files ./src/main",
    "start": "node dist/main",
    "migration:create": "create-migration ./src/db",
    "migration:generate": "generate-migration ./src/db",
    "migration:run": "run-all-migrations ./src/db",
    "migration:revert": "revert-migration ./src/db",
    "seed:create": "NODE_ENV=development create-seed ./src/db",
    "seed:run-all": "NODE_ENV=development run-all-seeds ./src/db"
  },
  "dependencies": {
    "@fastify/static": "^8.2.0",
    "@langchain/core": "^0.3.72",
    "@langchain/langgraph": "^0.4.6",
    "@langchain/langgraph-checkpoint": "^0.1.1",
    "@langchain/openai": "^0.6.9",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@packages/common": "workspace:*",
    "@packages/http-server": "workspace:*",
    "@packages/metrics": "workspace:*",
    "@packages/typeorm": "workspace:*",
    "@tavily/core": "^0.5.11",
    "axios": "^1.11.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.6.0",
    "dockerode": "^4.0.7",
    "dotenv": "^17.2.1",
    "enum-values": "^1.2.1",
    "fastify": "^5.5.0",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "nestjs-zod": "^5.0.1",
    "openai": "^5.15.0",
    "pg": "^8.16.3",
    "pino": "^9.9.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.26",
    "uuid": "^11.1.0",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@hey-api/client-axios": "^0.9.1",
    "@nestjs/testing": "^11.1.6",
    "@packages/cypress": "workspace:*",
    "@types/dockerode": "^3.3.26",
    "@types/lodash": "^4.17.20",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "cypress": "^14.5.4",
    "eslint": "^9.33.0",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^3.3.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "typescript-nestjs-swagger-plugin": "^2.0.0",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  }
}
