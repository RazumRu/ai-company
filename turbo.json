{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["tsconfig.json", "tsconfig.build.json"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**"],
      "outputs": ["dist/**"]
    },
    "build:tests": {
      "dependsOn": ["^build"],
      "inputs": ["src/**"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": [],
      "inputs": ["src/**"]
    },
    "lint:fix": {
      "outputs": [],
      "inputs": ["src/**"]
    },
    "test": {
      "inputs": ["src/**"]
    },
    "test:cov": {
      "inputs": ["src/**"]
    },
    "test:e2e": {
    },
    "test:e2e:local": {},
    "start:dev": {}
  }
}
